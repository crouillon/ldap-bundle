{
    "name": "lp-digital/ldap-bundle",
    "description": "LDAP authentication handler for BackBee v1.* instance",
    "type": "backbee-bundle",
    "license": "GPL-3.0+",
    "keywords": ["backbee", "ldap", "security", "bundle"],
    "authors": [
        {
            "name": "Charles Rouillon",
            "email": "charles.rouillon@lp-digital.fr"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-ldap": "*",
        "backbee/backbee-php": "~1.3.0",
        "symfony/ldap": "~3.2.0"
    },
    "autoload": {
        "psr-4": {
            "LpDigital\\Bundle\\LdapBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LpDigital\\Bundle\\LdapBundle\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6.0",
        "mikey179/vfsStream": "~1.6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}